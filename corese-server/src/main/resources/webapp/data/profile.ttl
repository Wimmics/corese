st:shex a st:Server ;
    st:service "shex" ;
    st:workflow st:shex  ;
    st:param [
        st:title "Shex" ;
        st:comment "Shex Shacl Translator with Shex parser <a href='http://shexjava.lille.inria.fr/'>http://shexjava.lille.inria.fr</a>" ;
        st:function sh:shex ;
        st:schema <shape/human.shex>;
        st:rdf    <shape/human.ttl>
    ] ;
